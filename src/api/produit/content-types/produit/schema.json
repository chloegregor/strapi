{
  "kind": "collectionType",
  "collectionName": "produits",
  "info": {
    "singularName": "produit",
    "pluralName": "produits",
    "displayName": "Produit",
    "description": "Organize your content into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "composition": {
      "type": "component",
      "component": "shared.composition-item",
      "repeatable": true,
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "entretien": {
      "type": "text",
      "required": true
    },
    "produit_couleurs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::produit-couleur.produit-couleur",
      "mappedBy": "produit"
    }
  }
}
